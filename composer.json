{
    "name": "packages/laravel-sso",
    "description": "Laravel SSO package for cross-site authentication between partner applications",
    "type": "library",
    "keywords": ["laravel", "sso", "authentication", "cross-site", "single-sign-on"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/encryption": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/auth": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Packages\\LaravelSSO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Packages\\LaravelSSO\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packages\\LaravelSSO\\Providers\\SSOServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
